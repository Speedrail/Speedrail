{
  "expo": {
    "name": "Speedrail",
    "slug": "Speedrail",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/speedrail.png",
    "scheme": "speedrail",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.fishnos.Speedrail",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      },
      "config": {
        "googleMapsApiKey": "${GOOGLE_MAPS_API_KEY}"
      }
    },
    "notification": {
      "icon": "./assets/images/speedrail.png",
      "color": "#6a99e3",
      "iosDisplayInForeground": true
    },
    "android": {
      "icon": "./assets/images/speedrail.png",
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.fishnos.Speedrail",
      "config": {
        "googleMaps": {
          "apiKey": "${GOOGLE_MAPS_API_KEY}"
        }
      }
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/speedrail.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/images/speedrail.png",
          "color": "#6a99e3",
          "sounds": []
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "6283cfce-cbba-42bb-bfa9-985893ea31dc"
      }
    }
  }
}
